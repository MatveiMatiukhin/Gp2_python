      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "import re\n",
        "from collections import defaultdict\n",
        "import logging\n",
        "\n",
        "\n",
        "\n",
        "logging.basicConfig(level=logging.INFO,format=\"%(asctime)s [%(levelname)s] %(message)s\",)\n",
        "logger = logging.getLogger(__name__)\n",
        "\n",
        "\n",
        "API_key = \""\n",
        "BASE_URL = \"https://ws.audioscrobbler.com/2.0/\"\n",
        "# Кэши чтобы не дергать по несколько раз API\n",
        "genre_desc_cache = {}\n",
        "artist_tags_cache = {}\n",
        "artist_info_cache = {}\n",
        "\n",
        "#Словарь правил нормализации жанров\n",
        "GENRE_RULES = [\n",
        "    (['k-pop', 'kpop', 'korean pop'], 'k-pop'),\n",
        "    (['j-pop', 'jpop', 'japanese pop'], 'j-pop'),\n",
        "    (['c-pop', 'cpop', 'chinese pop'], 'c-pop'),\n",
        "    (['hip hop', 'hip-hop', 'hiphop'], 'hip hop'),\n",
        "    (['r&b', 'rnb', 'r&b/soul', 'soul and rnb'], 'r&b'),\n",
        "    (['dance pop', 'dance-pop'], 'dance pop'),\n",
        "    (['pop rock'], 'pop rock'),\n",
        "    (['alt rock', 'alternative rock'], 'alternative rock'),\n",
        "    (['indie rock'], 'indie rock'),\n",
        "    (['indie pop'], 'indie pop'),\n",
        "    (['alt pop', 'alternative pop'], 'alternative pop'),\n",
        "    (['electro pop', 'electropop'], 'electropop'),\n",
        "    (['synth pop', 'synthpop'], 'synthpop'),\n",
        "    (['dream pop'], 'dream pop'),\n",
        "    (['bedroom pop'], 'bedroom pop'),\n",
        "    (['hard rock'], 'hard rock'),\n",
        "    (['psychedelic rock'], 'psychedelic rock'),\n",
        "    (['punk rock'], 'punk rock'),\n",
        "    (['metalcore'], 'metalcore'),\n",
        "    (['heavy metal'], 'heavy metal'),\n",
        "    (['black metal'], 'black metal'),\n",
        "    (['death metal'], 'death metal'),\n",
        "    (['indie'], 'indie'),\n",
        "    (['alternative'], 'alternative'),\n",
        "    (['rock'], 'rock'),\n",
        "    (['metal'], 'metal'),\n",
        "    (['pop'], 'pop'),\n",
        "    (['rap'], 'rap'),\n",
        "    (['trap'], 'trap'),\n",
        "    (['phonk'], 'phonk'),\n",
        "    (['drill'], 'drill'),\n",
        "    (['edm'], 'edm'),\n",
        "    (['electronic'], 'electronic'),\n",
        "    (['techno'], 'techno'),\n",
        "    (['house'], 'house'),\n",
        "    (['deep house'], 'deep house'),\n",
        "    (['progressive house'], 'progressive house'),\n",
        "    (['trance'], 'trance'),\n",
        "    (['dubstep'], 'dubstep'),\n",
        "    (['drum and bass', 'dnb'], 'drum and bass'),\n",
        "    (['lofi', 'lo-fi'], 'lofi'),\n",
        "    (['jazz'], 'jazz'),\n",
        "    (['blues'], 'blues'),\n",
        "    (['soul'], 'soul'),\n",
        "    (['funk'], 'funk'),\n",
        "    (['disco'], 'disco'),\n",
        "    (['punk'], 'punk'),\n",
        "    (['emo'], 'emo'),\n",
        "    (['hardcore'], 'hardcore'),\n",
        "    (['ska'], 'ska'),\n",
        "    (['reggae'], 'reggae'),\n",
        "    (['reggaeton'], 'reggaeton'),\n",
        "    (['latin'], 'latin'),\n",
        "    (['bossa nova'], 'bossa nova'),\n",
        "    (['mpb'], 'mpb'),\n",
        "    (['folk'], 'folk'),\n",
        "    (['country'], 'country'),\n",
        "    (['singer-songwriter', 'singer songwriter'], 'singer-songwriter'),\n",
        "    (['acoustic'], 'acoustic'),\n",
        "    (['ambient'], 'ambient'),\n",
        "    (['soundtrack', 'ost'], 'soundtrack'),\n",
        "    (['classical'], 'classical'),\n",
        "    (['orchestral', 'orchestra'], 'classical'),\n",
        "    (['opera'], 'opera'),\n",
        "    (['gospel'], 'gospel'),\n",
        "]\n",
        "\n",
        "def call_api(params: dict): #Делает запрос к API Last.fm и возвращает ответ в виде словаря JSON\n",
        "\n",
        "    method = params.get(\"method\", \"unknown\")\n",
        "    resp = requests.get(BASE_URL, params=params)\n",
        "    logger.info(f\"API call: {method}, status={resp.status_code}\")\n",
        "\n",
        "    if resp.status_code != 200:\n",
        "        return None\n",
        "    return resp.json()\n",
        "\n",
        "def canonical_genre_from_tag(raw_tag): # принимает \"сырой\" тег из Last.fm и возвращает каноническое имя жанра\n",
        "    if not isinstance(raw_tag, str): # если пришло не строка (например, None или число)- сразу говорим \"это не жанр\".\n",
        "        return None\n",
        "    g = raw_tag.strip() # убираем пробелы по краям; пустая строка — не жанр.\n",
        "    if not g:\n",
        "        return None\n",
        "\n",
        "    g_low = g.lower() # переводим в нижний регистр, чтобы сопоставлять без учёта регистра.\n",
        "\n",
        "    bad_substrings = [ ' fm', ' radio', 'records', 'record label', 'playlist', ' station',\n",
        "        'chart', 'favorite', 'favourite', 'study', 'sleep', 'wsum', 'pop court',\n",
        "        'tiktok', 'party', 'mix', 'hits', 'best of', 'version', 'edit' ] # фильтр мусора: если тег похож на плейлист/радио/лейбл/чарт/микс и т.п, отбрасываем\n",
        "    if any(sub in g_low for sub in bad_substrings):\n",
        "        return None\n",
        "\n",
        "\n",
        "    award_like = ['soty', 'song of the year', 'sing of the year'] # фильтруем наградные/фановые теги: они не про жанры\n",
        "    if any(tok in g_low for tok in award_like):\n",
        "        return None\n",
        "\n",
        "\n",
        "    if re.search(r'\\d', g_low) and not any(dec in g_low for dec in ['80s', '90s', '00s', '70s', '60s']):\n",
        "        return None # если в теге есть цифры, то это почти всегда не жанр (год, номер чарта и т.д),кроме классических десятилетий: 60s/70s/80s/90s/00s\n",
        "\n",
        "    base = re.sub(r'[\\(\\)\\[\\]\\{\\}]', ' ', g_low) # нормализация текста, все скобки - пробелы\n",
        "    base = base.replace('&', ' and ') # заменяем & на and (чтобы биться с правилами r&b/r and b и т.п)\n",
        "    base = re.sub(r'\\s+', ' ', base).strip() # схлопываем повторные пробелы.\n",
        "\n",
        "    for keys, canon in GENRE_RULES: # пробегаем по таблице правил GENRE_RULES:\n",
        "        for key in keys:\n",
        "            if key in base:\n",
        "                return canon\n",
        "    return None\n",
        "\n",
        "\n",
        "def get_genre_description(genre_name): # функция принимает имя жанра и должна вернуть краткое описание жанра (текст)\n",
        "    if genre_name in genre_desc_cache:\n",
        "        return genre_desc_cache[genre_name]\n",
        "\n",
        "    data = call_api({\"method\": \"tag.getinfo\", \"tag\": genre_name,\"api_key\": API_key,\"format\": \"json\"})\n",
        "    if data is None:                                        # если нет ответа от API\n",
        "        result = \"No genre information available\"\n",
        "    else:\n",
        "        result = data.get(\"tag\", {}).get(\"name\", \"Unknown genre\")\n",
        "\n",
        "    genre_desc_cache[genre_name] = result\n",
        "    return result\n",
        "\n",
        "def get_artist_tags(artist_name): # получает топ-теги (жанровые и прочие) для артиста по имени\n",
        "    if artist_name in artist_tags_cache:\n",
        "        return artist_tags_cache[artist_name]\n",
        "\n",
        "    data = call_api({\"method\": \"artist.gettoptags\",\"artist\": artist_name, \"api_key\": API_key, \"format\": \"json\",\"autocorrect\": 1})\n",
        "\n",
        "    tags_list = data.get(\"toptags\", {}).get(\"tag\", [])\n",
        "    if isinstance(tags_list, dict):\n",
        "        tags_list = [tags_list]\n",
        "\n",
        "    artist_tags_cache[artist_name] = tags_list\n",
        "    return tags_list\n",
        "\n",
        "def get_artist_info(artist_name):  # возвращаем базовую статистику артиста (listeners, playcount)\n",
        "    if artist_name in artist_info_cache:\n",
        "        return artist_info_cache[artist_name]\n",
        "\n",
        "    data = call_api({\"method\": \"artist.getinfo\",\"artist\": artist_name,\"api_key\": API_key,\"format\": \"json\",\"autocorrect\": 1})\n",
        "    stats = data.get(\"artist\", {}).get(\"stats\", {})\n",
        "    artist_listeners = stats.get(\"listeners\", \"\")\n",
        "    artist_playcount = stats.get(\"playcount\", \"\")\n",
        "\n",
        "    artist_info_cache[artist_name] = (artist_listeners, artist_playcount)\n",
        "    return artist_info_cache[artist_name]\n",
        "\n",
        "def choose_genre_from_tags(track_tags, artist_tags): # выбираем один \"лучший\" жанр по тегам трека и артиста\n",
        "\n",
        "    scores = defaultdict(int)  # словарь - суммарный вес\n",
        "\n",
        "    for tag in track_tags: # сначала обрабатываем теги конкретного трека\n",
        "        name = tag.get(\"name\", \"\")\n",
        "        canon = canonical_genre_from_tag(name)\n",
        "        try:\n",
        "            w = int(tag.get(\"count\", 1)) # вес тега (Last.fm даёт count как строку)\n",
        "        except ValueError:\n",
        "            w = 1  # если не число — считаем вес 1\n",
        "        scores[canon] += w * 2 # трек важнее артиста - удваиваем вклад\n",
        "\n",
        "    for tag in artist_tags: # обрабатываем теги Артиста\n",
        "        name = tag.get(\"name\", \"\")\n",
        "        canon = canonical_genre_from_tag(name)\n",
        "        try:\n",
        "            w = int(tag.get(\"count\", 1))\n",
        "        except ValueError:\n",
        "            w = 1\n",
        "        scores[canon] += w\n",
        "\n",
        "    return max(scores.items(), key=lambda x: x[1])[0] # берём жанр с максимальным весом и возвращаем его имя\n",
        "\n",
        "def get_track_info(artist_name, track_name): # получаем инфо по треку: длительность, жанр, описание жанра\n",
        "\n",
        "    duration_min = None\n",
        "    genre = None\n",
        "    genre_description = \"\"\n",
        "\n",
        "    data = call_api({\"method\": \"track.getInfo\",\"artist\": artist_name,\"track\": track_name,\"api_key\": API_key,\"format\": \"json\",\"autocorrect\": 1})\n",
        "    tr = data.get(\"track\")\n",
        "    duration_raw = tr.get(\"duration\", \"\")\n",
        "    try:\n",
        "           ms = int(duration_raw)\n",
        "           tmp_min = ms / 60000.0\n",
        "           if 1.0 <= tmp_min <= 15.0:\n",
        "             duration_min = tmp_min\n",
        "    except ValueError:\n",
        "            pass\n",
        "\n",
        "\n",
        "    track_tags = tr.get(\"toptags\", {}).get(\"tag\", []) # теги привязанные к самому треку\n",
        "    artist_tags = get_artist_tags(artist_name)\n",
        "\n",
        "    genre = choose_genre_from_tags(track_tags, artist_tags)\n",
        "    if genre:\n",
        "        genre_description = get_genre_description(genre)\n",
        "\n",
        "    return duration_min, genre, genre_description\n",
        "\n",
        "def format_duration_human(minutes): # переводим длительность в читаему строку\n",
        "    total_seconds = int(round(minutes * 60))\n",
        "    m, s = divmod(total_seconds, 60)\n",
        "    if m == 0:\n",
        "        return f\"{s} сек\"\n",
        "    if s == 0:\n",
        "        return f\"{m} мин\"\n",
        "    return f\"{m} мин {s:02d} сек\"\n",
        "\n",
        "\n",
        "logger.info(\"Старт сбора данных из Last.fm chart.getTopTracks\")\n",
        "\n",
        "rows = []\n",
        "\n",
        "for page in range(1, 5):\n",
        "    data = call_api({\"method\": \"chart.gettoptracks\",\"api_key\": API_key,\"format\": \"json\",\"limit\": 500, \"page\": page})\n",
        "    if data is None:\n",
        "        logger.info(f\"Страница {page}: ошибка ответа, пропускаем\")\n",
        "        continue\n",
        "\n",
        "    tracks = data.get(\"tracks\", {}).get(\"track\", [])\n",
        "    logger.info(f\"Страница {page}: получено треков — {len(tracks)}\")\n",
        "\n",
        "    for t in tracks:\n",
        "        artist = t.get(\"artist\", {})\n",
        "        artist_name = artist.get(\"name\", \"\")\n",
        "        track_name = t.get(\"name\", \"\")\n",
        "\n",
        "        duration_min, genre, genre_description = get_track_info(artist_name, track_name)\n",
        "        artist_listeners, artist_playcount = get_artist_info(artist_name)\n",
        "\n",
        "        rows.append({ \"Artist\": artist_name,\"Song\": track_name,\n",
        "            \"listeners\": t.get(\"listeners\", \"\"),\n",
        "            \"playcount\": t.get(\"playcount\", \"\"),\n",
        "            \"track_url\": t.get(\"url\", \"\"),\n",
        "            \"artist_url\": artist.get(\"url\", \"\"),\n",
        "            \"artist_mbid\": artist.get(\"mbid\", \"\"),\n",
        "            \"duration_min\": duration_min,\n",
        "            \"genre\": genre,\n",
        "            \"genre_description\": genre_description,\n",
        "            \"artist_listeners\": artist_listeners,\n",
        "            \"artist_playcount\": artist_playcount\n",
        "        })\n",
        "\n",
        "df_api = pd.DataFrame(rows)\n",
        "logger.info(f\"Всего строк до фильтрации: {df_api.shape[0]}\")\n",
        "\n",
        "\n",
        "df_api = df_api.dropna(subset=[\"duration_min\", \"genre\"])\n",
        "logger.info(f\"После фильтра по duration_min и genre: {df_api.shape[0]} строк\")\n",
        "\n",
        "\n",
        "df_api[\"playcount_num\"] = pd.to_numeric(df_api[\"playcount\"], errors=\"coerce\")\n",
        "df_api = df_api.dropna(subset=[\"playcount_num\"])\n",
        "df_api = df_api.sort_values(\"playcount_num\", ascending=False)\n",
        "df_api[\"rank\"] = range(1, len(df_api) + 1)\n",
        "df_api = df_api.drop(columns=[\"playcount_num\"])\n",
        "\n",
        "\n",
        "df_api[\"duration\"] = df_api[\"duration_min\"].apply(format_duration_human)\n",
        "\n",
        "\n",
        "cols_order = [\"Artist\", \"Song\",\"listeners\", \"playcount\", \"rank\",\"track_url\", \"artist_url\", \"artist_mbid\",\"duration_min\", \"duration\",\"genre\", \"genre_description\",\"artist_listeners\", \"artist_playcount\"]\n",
        "df_api = df_api[cols_order]\n",
        "\n",
        "df_api.to_csv(\"df_api.csv\", index=False)\n",
        "logger.info(f\"Финальный датасет сохранён в df_api.csv, shape={df_api.shape}\")\n",
        "\n"
      ],
